#openaddresses {
  marker-fill:#f87244;
  marker-fill-opacity:0.4;
  marker-line-width:0;
  marker-allow-overlap:true;
  marker-width:2px;
  [zoom>=5] { marker-fill-opacity:0.10; }
  [zoom>=10] { marker-fill-opacity:0.5; }
  [zoom>=14] { marker-fill-opacity:0.75; }
  [zoom>=15] { marker-width:3px; }
  [zoom>=16] { marker-fill-opacity:1; marker-width:4px; }
  [zoom>=17] { marker-width:5px; }
}