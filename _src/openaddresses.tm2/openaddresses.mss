#openaddresses {
  marker-fill:#f87244;
  marker-fill-opacity:0.4;
  marker-line-width:0;
  marker-allow-overlap:true;
  marker-width:2.5px;
  [zoom>=5] { marker-fill-opacity:0.2; marker-width: 3.2px; }
  [zoom>=7] { marker-fill-opacity:0.3; marker-width: 3.4px; }
  [zoom>=10] { marker-fill-opacity:0.5; marker-width: 3.5px; }
  [zoom>=14] { marker-fill-opacity:0.75; marker-width: 3.7px; }
  [zoom>=15] { marker-width: 3.3px; }
  [zoom>=16] { marker-fill-opacity:1; marker-width:4px; }
  [zoom>=17] { marker-width:5px; }
}