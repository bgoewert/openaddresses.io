<div class="editor">

    <div class="toolbar">
        <div class="tool saveTool"></div>
        <div class="tool closeTool"></div>
    </div>

    <h1>{{filename}}</h1>

    <div class="pane">
        <div class="paneTitle">Data:<div class="helpIcon"></div></div>
        <div class="inputContainer">
            <input id="hoverEdit" type="text" name="input" value="{{data}}"></input>
            <div class="status save"></div>
        </div>

        <div class="paneTitle">Website:<div class="helpIcon"></div></div>
        <div class="inputContainer">
            <input id="hoverEdit" type="text" name="input" value="{{website}}"></input>
            <div class="status save"></div>
        </div>

        <div class="paneTitle">Attribution:<div class="helpIcon"></div></div>
        <div class="inputContainer">
            <input id="hoverEdit" type="text" name="input" value="{{attribution}}"></input>
            <div class="status save"></div>
        </div>

        <div class="paneTitle">Type:<div class="helpIcon"></div></div>
        <div class="inputContainer">
            <select>
                <option value="esri">ESRI</option>
                <option value="ftp">FTP</option>
                <option value="http">HTTP(S)</option>
            </select>
            <div class="status edit"></div>
        </div>

        <div class="paneTitle">Compression:<div class="helpIcon"></div></div>
        <div class="inputContainer">
            <select>
                <option value="zip">.zip</option>
                <option value="none">none</option>
            </select>
            <div class="status edit"></div>
        </div>
    </div>
</div>
